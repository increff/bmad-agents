.env
.env.local
.env.*.local

# Build and dependency directories
node_modules/
dist/
build/
target/

# Logs and temporary files
*.log
logs/
tmp/
temp/

# System files
.DS_Store
Thumbs.db

# IDE and editor files
.vscode/
.idea/
*.swp
*.swo

# AI assistant files
CLAUDE.md
.ai/
.claude
.gemini
.iflow

# BMAD specific
.bmad-core
.bmad-creator-tools
.bmad*/

# Project-specific generated files
*.stats.md
flattened-codebase.xml

# Test and sample directories
test-project-install/
sample-project/
